qt_libkawkawqt_a_SOURCES = $(KAWKAW_QT_CPP) $(KAWKAW_QT_H) $(QT_FORMS_UI) \
  $(QT_QRC) $(QT_QRC_LOCALE) $(QT_TS) $(PROTOBUF_PROTO) $(RES_ICONS) $(RES_IMAGES) $(RES_MOVIES)

nodist_qt_libkawkawqt_a_SOURCES = $(QT_MOC_CPP) $(QT_MOC) $(PROTOBUF_CC) \
  $(PROTOBUF_H) $(QT_QRC_CPP) $(QT_QRC_LOCALE_CPP)

# forms/foo.h -> forms/ui_foo.h
QT_FORMS_H=$(join $(dir $(QT_FORMS_UI)),$(addprefix ui_, $(notdir $(QT_FORMS_UI:.ui=.h))))

# Most files will depend on the forms and moc files as includes. Generate them
# before anything else.
$(QT_MOC): $(QT_FORMS_H)
$(qt_libkawkawqt_a_OBJECTS) $(qt_kawkaw_qt_OBJECTS) : | $(QT_MOC)

#Generating these with a half-written protobuf header leads to wacky results.
#This makes sure it's done.
$(QT_MOC): $(PROTOBUF_H)
$(QT_MOC_CPP): $(PROTOBUF_H)

# kawkaw-qt binary #
qt_kawkaw_qt_CPPFLAGS = $(AM_CPPFLAGS) $(KAWKAW_INCLUDES) $(KAWKAW_QT_INCLUDES) \
  $(QT_INCLUDES) $(PROTOBUF_CFLAGS) $(QR_CFLAGS)
qt_kawkaw_qt_CXXFLAGS = $(AM_CXXFLAGS) $(QT_PIE_FLAGS)

qt_kawkaw_qt_SOURCES = qt/kawkaw.cpp
if TARGET_DARWIN
  qt_kawkaw_qt_SOURCES += $(KAWKAW_MM)
endif
if TARGET_WINDOWS
  qt_kawkaw_qt_SOURCES += $(KAWKAW_RC)
endif
qt_kawkaw_qt_LDADD = qt/libkawkawqt.a $(LIBKAWKAW_SERVER)
if ENABLE_WALLET
qt_kawkaw_qt_LDADD += $(LIBKAWKAW_WALLET)
endif
if ENABLE_ZMQ
qt_kawkaw_qt_LDADD += $(LIBKAWKAW_ZMQ) $(ZMQ_LIBS)
endif
qt_kawkaw_qt_LDADD += $(LIBkawkaw_cli) $(LIBKAWKAW_COMMON) $(LIBKAWKAW_UTIL) $(LIBKAWKAW_CONSENSUS) $(LIBKAWKAW_CRYPTO) $(LIBUNIVALUE) $(LIBLEVELDB) $(LIBLEVELDB_SSE42) $(LIBMEMENV) \
  $(BOOST_LIBS) $(QT_LIBS) $(QT_DBUS_LIBS) $(QR_LIBS) $(PROTOBUF_LIBS) $(BDB_LIBS) $(SSL_LIBS) $(CRYPTO_LIBS) $(MINIUPNPC_LIBS) $(LIBSECP256K1) \
  $(EVENT_PTHREADS_LIBS) $(EVENT_LIBS)
qt_kawkaw_qt_LDFLAGS = $(RELDFLAGS) $(AM_LDFLAGS) $(QT_LDFLAGS) $(LIBTOOL_APP_LDFLAGS)
qt_kawkaw_qt_LIBTOOLFLAGS = --tag CXX

#locale/foo.ts -> locale/foo.qm
QT_QM=$(QT_TS:.ts=.qm)

SECONDARY: $(QT_QM)

$(srcdir)/qt/kawkawstrings.cpp: $(libkawkaw_server_a_SOURCES) $(libkawkaw_wallet_a_SOURCES) $(libkawkaw_common_a_SOURCES) $(libkawkaw_zmq_a_SOURCES) $(libkawkaw_consensus_a_SOURCES) $(libkawkaw_util_a_SOURCES)
	@test -n $(XGETTEXT) || echo "xgettext is required for updating translations"
	$(AM_V_GEN) cd $(srcdir); XGETTEXT=$(XGETTEXT) PACKAGE_NAME="$(PACKAGE_NAME)" COPYRIGHT_HOLDERS="$(COPYRIGHT_HOLDERS)" COPYRIGHT_HOLDERS_SUBSTITUTION="$(COPYRIGHT_HOLDERS_SUBSTITUTION)" $(PYTHON) ../share/qt/extract_strings_qt.py $^

translate: $(srcdir)/qt/kawkawstrings.cpp $(QT_FORMS_UI) $(QT_FORMS_UI) $(KAWKAW_QT_BASE_CPP) qt/kawkaw.cpp $(KAWKAW_QT_WINDOWS_CPP) $(KAWKAW_QT_WALLET_CPP) $(KAWKAW_QT_H) $(KAWKAW_MM)
	@test -n $(LUPDATE) || echo "lupdate is required for updating translations"
	$(AM_V_GEN) QT_SELECT=$(QT_SELECT) $(LUPDATE) $^ -locations relative -no-obsolete -ts $(srcdir)/qt/locale/kawkaw_en.ts

$(QT_QRC_LOCALE_CPP): $(QT_QRC_LOCALE) $(QT_QM)
	@test -f $(RCC)
	@cp -f $< $(@D)/temp_$(<F)
	$(AM_V_GEN) QT_SELECT=$(QT_SELECT) $(RCC) -name kawkaw_locale $(@D)/temp_$(<F) | \
	  $(SED) -e '/^\*\*.*Created:/d' -e '/^\*\*.*by:/d' > $@
	@rm $(@D)/temp_$(<F)

$(QT_QRC_CPP): $(QT_QRC) $(QT_FORMS_H) $(RES_ICONS) $(RES_IMAGES) $(RES_MOVIES) $(PROTOBUF_H)
	@test -f $(RCC)
	$(AM_V_GEN) QT_SELECT=$(QT_SELECT) $(RCC) -name kawkaw $< | \
	  $(SED) -e '/^\*\*.*Created:/d' -e '/^\*\*.*by:/d' > $@

